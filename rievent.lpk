      <Item>
        <Filename Value="readdevinput_gamepadsnes.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInput_GamepadSNES"/>
      </Item>
      <Item>
        <Filename Value="readdevinput_vecpedal.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInput_VecPedal"/>
      </Item>
      <Item>
        <Filename Value="readdevinputcomp_mouse.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInputComp_Mouse"/>
      </Item>
      <Item>
        <Filename Value="readdevinputcomp_keyboard.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInputComp_Keyboard"/>
      </Item>
    </Files>
    <RequiredPkgs>
