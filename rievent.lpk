<?xml version="1.0" encoding="UTF-8"?>
<CONFIG>
  <Package Version="5">
    <Name Value="RIEvent"/>
    <Type Value="RunAndDesignTime"/>
    <CompilerOptions>
      <Version Value="11"/>
      <SearchPaths>
        <UnitOutputDirectory Value="lib/$(TargetCPU)-$(TargetOS)"/>
      </SearchPaths>
    </CompilerOptions>
    <Files>
      <Item>
        <Filename Value="readdevinput.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInput"/>
      </Item>
      <Item>
        <Filename Value="readdevinput_contourshuttlepro.pas"/>
        <HasRegisterProc Value="True"/>
        <UnitName Value="ReadDevInput_ContourShuttlePro"/>
